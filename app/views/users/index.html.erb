<%#edit_user_path あと、controllerで未使用のインスタンス変数があってもエラーは出ないらしい。」%>
<%= render 'books/list', user: current_user %>

<div class="col-md-8 offset-md-1">
  <h2>Users</h2>
    <table class="table">
      <thead>
        <tr>
          <th>image</th>
          <th>name</th>
          <th></th>
        </tr>
      </thead>
      <% @users.each do |user| %>
      <tbody>
        <tr>
          <td><%= attachment_image_tag user, :profile_image, :fill, 60, 60 %></td>
          <td><%= user.name %></td>
          <td><%= link_to "show", user_path(user.id) %></td>
        </tr>
      </body>
    </table>
  <% end %>
</ul>
</div>